{
    "disable_update_check": {{disable_update_check}},
    "encrypt": "{{encryption_key}}",
    "datacenter": "{{datacenter}}",
    "bind_addr": "{{local_ip}}",
    "client_addr": "{{consul_interface_ip}}",
    "raft_protocol": 3,
    "enable_syslog": true,
    "acl": {
        "enabled": true,
        {% if not agent_acl_token.firstrun %}
            "tokens": {
                "agent": "{{agent_acl_token}}",
                "default": "{{agent_acl_token}}"
            }
        {% endif %}
    }
}
